<Window title="Pivot &quot;Kitchen Sink&quot; Demo" maximized="true"
    xmlns:bxml="http://pivot.apache.org/bxml"
    xmlns:effects="org.apache.pivot.wtk.effects"
    xmlns="org.apache.pivot.wtk"
>
    <Border styles="{backgroundColor:10, thickness:0, padding:4}">
        <Border>
            <ScrollPane bxml:id="scrollPane" styles="{backgroundColor:11}"
                horizontalScrollBarPolicy="auto"
                verticalScrollBarPolicy="auto">
                <BoxPane orientation="vertical" styles="{padding:6}">
                    <Rollup bxml:id="infoRollup" styles="{buttonColor:13}" enabled="false" tooltipText="Rollup element disabled sample">
                        <heading>
                            <Label text="Info" styles="{font:{size:'110%', bold:true}, color:13}" enabled="false"/>
                        </heading>
                    </Rollup>
                    <Rollup bxml:id="buttonsRollup" styles="{buttonColor:13}" tooltipText="Rollup element for Buttons sample">
                        <heading>
                            <Label text="Buttons" styles="{font:{size:'110%', bold:true}, color:13}"/>
                        </heading>
                    </Rollup>
                    <Rollup bxml:id="listsRollup" styles="{buttonColor:13}">
                        <heading>
                            <Label text="Lists" styles="{font:{size:'110%', bold:true}, color:13}"/>
                        </heading>
                    </Rollup>
                    <Rollup bxml:id="textRollup" styles="{buttonColor:13}">
                        <heading>
                            <Label text="Text" styles="{font:{size:'110%', bold:true}, color:13}"/>
                        </heading>
                    </Rollup>
                    <Rollup bxml:id="calendarsRollup" styles="{buttonColor:13}">
                        <heading>
                            <Label text="Calendars" styles="{font:{size:'110%', bold:true}, color:13}"/>
                        </heading>
                    </Rollup>
                    <Rollup bxml:id="colorChoosersRollup" styles="{buttonColor:13}">
                        <heading>
                            <Label text="Color Choosers" styles="{font:{size:'110%', bold:true}, color:13}"/>
                        </heading>
                    </Rollup>
                    <Rollup bxml:id="navigationRollup" styles="{buttonColor:13}">
                        <heading>
                            <Label text="Navigation" styles="{font:{size:'110%', bold:true}, color:13}"/>
                        </heading>
                    </Rollup>
                    <Rollup bxml:id="splittersRollup" styles="{buttonColor:13}">
                        <heading>
                            <Label text="Splitters" styles="{font:{size:'110%', bold:true}, color:13}"/>
                        </heading>
                    </Rollup>
                    <Rollup bxml:id="menusRollup" styles="{buttonColor:13}">
                        <heading>
                            <Label text="Menus" styles="{font:{size:'110%', bold:true}, color:13}"/>
                        </heading>
                    </Rollup>
                    <Rollup bxml:id="metersRollup" styles="{buttonColor:13}">
                        <heading>
                            <Label text="Meters, Gauges &amp; Activity Indicators" styles="{font:{size:'110%', bold:true}, color:13}"/>
                        </heading>
                    </Rollup>
                    <Rollup bxml:id="spinnersRollup" styles="{buttonColor:13}">
                        <heading>
                            <Label text="Spinners &amp; Sliders" styles="{font:{size:'110%', bold:true}, color:13}"/>
                        </heading>
                    </Rollup>
                    <Rollup bxml:id="tablesRollup" styles="{buttonColor:13}">
                        <heading>
                            <Label text="Tables" styles="{font:{size:'110%', bold:true}, color:13}"/>
                        </heading>
                    </Rollup>
                    <Rollup bxml:id="treesRollup" styles="{buttonColor:13}">
                        <heading>
                            <Label text="Trees" styles="{font:{size:'110%', bold:true}, color:13}"/>
                        </heading>
                    </Rollup>
                    <Rollup bxml:id="dragDropRollup" styles="{buttonColor:13}">
                        <heading>
                            <Label text="Drag &amp; Drop" styles="{font:{size:'110%', bold:true}, color:13}"/>
                        </heading>
                    </Rollup>
                    <Rollup bxml:id="alertsRollup" styles="{buttonColor:13}">
                        <heading>
                            <Label text="Alerts &amp; Prompts" styles="{font:{size:'110%', bold:true}, color:13}"/>
                        </heading>
                    </Rollup>
                </BoxPane>
            </ScrollPane>
        </Border>
    </Border>
</Window>
