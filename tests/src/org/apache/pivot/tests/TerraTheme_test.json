    colorMultiplier: 0.2,

    themeIsDark: false,

    themeIsFlat: true,

    transitionEnabled: false,

    colors: [
        "#0a0a0a",
        "#ebebeb",
        "#999999",
        "#dcdcdc",
        "#004d99",
        "#0076eb",
